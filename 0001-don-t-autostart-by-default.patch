Subject: [PATCH] don't autostart by default

---
 src/tracker-store/tracker-store.desktop.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/tracker-store/tracker-store.desktop.in b/src/tracker-store/tracker-store.desktop.in
index 985d19f..01df6ef 100644
--- a/src/tracker-store/tracker-store.desktop.in
+++ b/src/tracker-store/tracker-store.desktop.in
@@ -5,7 +5,7 @@ Exec=gdbus call -e -d org.freedesktop.DBus -o /org/freedesktop/DBus -m org.freed
 Terminal=false
 Type=Application
 Categories=Utility;
-X-GNOME-Autostart-enabled=true
+X-GNOME-Autostart-enabled=false
 X-KDE-autostart-after=panel
 X-KDE-StartupNotify=false
 X-KDE-UniqueApplet=true
